{
  "expo": {
    "name": "recapit",
    "slug": "recapit",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash1.png",
      "resizeMode": "contain",
      "backgroundColor": "#9E2BD0"
    },
    "scheme": "com.recapit",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "plugins": ["expo-font",
    [
      "expo-notifications",
      {
        "icon": "./src/assets/notification-icon.png",
        "color": "#082849"
      }
    ]]
  }
}
